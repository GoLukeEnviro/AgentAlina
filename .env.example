# =============================================================================
# AgentElli Environment Configuration Template
# =============================================================================
# Copy this file to .env and fill in your actual values
# Never commit real credentials to version control!

# =============================================================================
# CORE APPLICATION SETTINGS
# =============================================================================
APP_NAME=AgentElli
APP_VERSION=1.0.0
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO
SECRET_KEY=your_secret_key_here_change_in_production

# =============================================================================
# DATABASE CONFIGURATIONS
# =============================================================================

# PostgreSQL (Primary Database)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=agentalina
POSTGRES_USER=agentalina_user
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_SSL_MODE=prefer
DATABASE_URL=postgresql://agentalina_user:your_postgres_password_here@localhost:5432/agentalina

# Redis (Caching & Session Store)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here
REDIS_DB=0
REDIS_URL=redis://:your_redis_password_here@localhost:6379/0

# Neo4j (Graph Database)
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password_here

# =============================================================================
# AI/LLM CONFIGURATIONS
# =============================================================================

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=4096

# Anthropic Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# Ollama (Local LLM)
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=llama2

# Hugging Face
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
HUGGINGFACE_MODEL=microsoft/DialoGPT-medium

# =============================================================================
# EXTERNAL SERVICE API KEYS
# =============================================================================

# News APIs
NEWS_API_KEY=your_news_api_key_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Weather API
WEATHER_API_KEY=your_weather_api_key_here
OPENWEATHER_API_KEY=your_openweather_api_key_here

# GitHub Integration
GITHUB_API_KEY=your_github_personal_access_token_here
GITHUB_WEBHOOK_SECRET=your_github_webhook_secret_here

# Search & AI Services
TAVILY_API_KEY=your_tavily_api_key_here
BRAVE_SEARCH_API_KEY=your_brave_search_api_key_here
SERPAPI_KEY=your_serpapi_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# =============================================================================
# CRYPTO TRADING SETTINGS
# =============================================================================
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here
COINBASE_API_KEY=your_coinbase_api_key_here
COINBASE_SECRET=your_coinbase_secret_here
TRADING_ENABLED=false
TRADING_MODE=paper
MAX_POSITION_SIZE=1000
RISK_TOLERANCE=0.02

# =============================================================================
# MONITORING & OBSERVABILITY
# =============================================================================

# Prometheus
PROMETHEUS_PORT=9090
PROMETHEUS_RETENTION=15d

# Grafana
GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=your_grafana_password_here

# Logging
LOKI_URL=http://localhost:3100
LOG_FORMAT=json
LOG_FILE_PATH=/var/log/agentalina/app.log

# Error Tracking
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ENVIRONMENT=development

# =============================================================================
# AGENT CONFIGURATIONS
# =============================================================================
AGENT_MAX_ITERATIONS=100
AGENT_TIMEOUT=300
AGENT_MEMORY_SIZE=1000
AGENT_LEARNING_RATE=0.001
AGENT_EXPLORATION_RATE=0.1
AGENT_BATCH_SIZE=32

# =============================================================================
# RESOURCE LIMITS
# =============================================================================
MAX_WORKERS=4
MAX_MEMORY_MB=2048
MAX_CPU_PERCENT=80
REQUEST_TIMEOUT=30
MAX_CONCURRENT_REQUESTS=100

# =============================================================================
# DEVELOPMENT & TESTING
# =============================================================================
TEST_DATABASE_URL=postgresql://test_user:test_pass@localhost:5432/agentalina_test
TEST_REDIS_URL=redis://localhost:6379/1
PYTEST_TIMEOUT=60
COVERAGE_THRESHOLD=80

# =============================================================================
# DOCKER & DEPLOYMENT
# =============================================================================
DOCKER_REGISTRY=your_docker_registry_here
DOCKER_IMAGE_TAG=latest
KUBERNETES_NAMESPACE=agentalina
HELM_CHART_VERSION=1.0.0

# Health Checks
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=10
HEALTH_CHECK_RETRIES=3

# Backup & Recovery
BACKUP_ENABLED=true
BACKUP_SCHEDULE="0 2 * * *"
BACKUP_RETENTION_DAYS=30
S3_BACKUP_BUCKET=your_s3_backup_bucket_here
S3_ACCESS_KEY=your_s3_access_key_here
S3_SECRET_KEY=your_s3_secret_key_here
S3_REGION=us-east-1

# =============================================================================
# FEATURE FLAGS
# =============================================================================
ENABLE_TRADING=false
ENABLE_SOCIAL_MEDIA=false
ENABLE_VOICE_INTERFACE=false
ENABLE_ADVANCED_ANALYTICS=true
ENABLE_AUTO_SCALING=false
ENABLE_RATE_LIMITING=true

# =============================================================================
# SECURITY SETTINGS
# =============================================================================
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRATION=3600
CORS_ORIGINS=http://localhost:3000,http://localhost:8080
CSRF_SECRET=your_csrf_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600

# SSL/TLS
SSL_CERT_PATH=/etc/ssl/certs/agentalina.crt
SSL_KEY_PATH=/etc/ssl/private/agentalina.key
SSL_ENABLED=false

# =============================================================================
# NOTIFICATION SETTINGS
# =============================================================================
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_email_password_here
SMTP_TLS=true

# Slack Integration
SLACK_BOT_TOKEN=your_slack_bot_token_here
SLACK_CHANNEL=#agentalina-alerts

# Discord Integration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_CHANNEL_ID=your_discord_channel_id_here

# =============================================================================
# END OF CONFIGURATION
# =============================================================================